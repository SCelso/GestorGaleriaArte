<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" minHeight="600.0" minWidth="1080.0" prefHeight="600.0" prefWidth="1080.0" spacing="20.0" style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nebrija.gestorgaleriaarte.FirstController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox alignment="CENTER" prefHeight="0.0" prefWidth="0.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="GALERIA DE ARTE">
               <font>
                  <Font name="Arial" size="70.0" />
               </font>
            </Text>
         </children>
      </HBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="btnVerGaleria" mnemonicParsing="false" prefHeight="45.0" prefWidth="300.0" text="Ver Galeria" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btnIniciarSesion" mnemonicParsing="false" prefHeight="45.0" prefWidth="300.0" text="Iniciar Sesion" textAlignment="CENTER" />
         </children>
      </VBox>
   </children>
</VBox>
